{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DefaultRetryPolicy": {
    "MaxRetries": 5,
    "RetryType": "Constant",
    "RetryInterval": 2
  },
  "ChuckNorrisService": {
    "BaseUrl": "https://api.chucknorris.io",
    "RetryPolicy": {
      "MaxRetries": 3,
      "RetryType": "Exponential",
      "BackoffExponentialBase": 2
    }
  },
  "System2": {
    "BaseUrl": "https://system2.example.com",
    "RetryPolicy": {
      "MaxRetries": 5,
      "RetryType": "Constant",
      "RetryInterval": 2
    }
  },
  "AllowedHosts": "*"
}
